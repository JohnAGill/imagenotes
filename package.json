{
  "name": "imagenotes",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "format:js": "npm run prettier -- \"src/**/*.{ts,tsx,js,jsx,css}\"",
    "postinstall": "react-native-schemes-manager all",
    "ios": "react-native run-ios",
    "lint": "eslint ./src --ext=tsx",
    "lint:fix": "eslint ./src --ext=tsx, --fix",
    "precommit": "lint-staged && npm run lint",
    "prettier": "prettier --single-quote --trailing-comma all --print-width 180 --write",
    "start": "react-native start",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{ts,tsx,js,jsx,css}": [
      "yarn lint:fix --"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "@react-native-community/google-signin": "^4.0.0",
    "@react-native-firebase/app": "^6.4.0",
    "@react-native-firebase/auth": "^6.4.0",
    "@types/lodash": "^4.14.149",
    "@types/react-router-native": "^5.1.0",
    "lodash": "^4.17.15",
    "native-base": "^2.13.12",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-camera": "^3.22.1",
    "react-native-keyboard-done-button": "https://github.com/JohnAGill/react-native-keyboard-done-button",
    "react-router-native": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "lint-staged": "^10.1.3",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react-native-schemes-manager": "^2.0.0",
    "react-test-renderer": "16.11.0",
    "sort-package-json": "^1.41.0",
    "typescript": "^3.8.3"
  },
  "xcodeSchemes": {
    "Release": [
      "Staging"
    ],
    "Debug": []
  }
}
